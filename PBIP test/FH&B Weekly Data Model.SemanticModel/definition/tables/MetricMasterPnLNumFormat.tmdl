	calculationGroup
		precedence: 1

		calculationItem Unformatted = SELECTEDMEASURE ()

		calculationItem Formatted = SELECTEDMEASURE ()

			formatStringDefinition =
					VAR ValFormatString =
					    SELECTEDVALUE ( MetricMasterPnL[ValFormatString] )
					VAR VarFormatString =
					    SELECTEDVALUE ( MetricMasterPnL[VarFormatString] )
					VAR PctVarFormatString =
					    SELECTEDVALUE ( MetricMasterPnL[PctVarFormatString] )
					VAR FormatToApply =
					    SWITCH (
					        TRUE (),
					        ISSELECTEDMEASURE ( [P&L], [P&L LW], [P&L QRF], [P&L FC], [P&L BUD], [P&L LY] ), ValFormatString,
					        ISSELECTEDMEASURE ( [P&L vs LW], [P&L vs QRF], [P&L vs FC], [P&L vs BUD], [P&L vs LY]), VarFormatString,
					        ISSELECTEDMEASURE ( [P&L vs LW %], [P&L vs QRF %], [P&L vs FC %], [P&L vs BUD %], [P&L vs LY %]), PctVarFormatString,
					        SELECTEDMEASUREFORMATSTRING ()
					    )
					RETURN
					    FormatToApply

