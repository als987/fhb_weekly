table OnlineMetricNumFormat
	lineageTag: 1f4419ba-f041-4675-a51b-a0a5180e0b75

	calculationGroup
		precedence: 5

		calculationItem Unformatted = SELECTEDMEASURE ()

		calculationItem Formatted = SELECTEDMEASURE ()

			formatStringDefinition =
					VAR FormatString =
					    SELECTEDVALUE ( OnlineMetrics[NumFormatString] )
					VAR FormatToApply =
					    SWITCH (
					        TRUE (),
					        ISSELECTEDMEASURE ( [SwitchOnlineMetric],  [SwitchOnlineMetric LW], [SwitchOnlineMetric vs LW] ), FormatString,
					        SELECTEDMEASUREFORMATSTRING ()
					    )
					RETURN
					    FormatToApply

		calculationItem ReturnFormat =
				VAR FormatString =
				    SELECTEDVALUE ( OnlineMetrics[NumFormatString] )
				VAR FormatToApply =
				    SWITCH (
				        TRUE (),
				        ISSELECTEDMEASURE ( [SwitchOnlineMetric],  [SwitchOnlineMetric LW], [SwitchOnlineMetric vs LW] ), FormatString,
				        SELECTEDMEASUREFORMATSTRING ()
				    )
				RETURN
				    FormatToApply

